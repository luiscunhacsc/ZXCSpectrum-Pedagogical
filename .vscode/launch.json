{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug ZX-Spectrum",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/zx48.exe",
        "args": [],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "externalConsole": true,
        "MIMode": "gdb",
        "miDebuggerPath": "E:/msys64/mingw64/bin/gdb.exe",
        "setupCommands": [
          {
            "description": "Enable pretty printing",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }
  